type Query {
  todos: [Todo!]!
}

type Todo {
  ID: String!
  Title: String!
  Description: String!
  Condition: Boolean!
}

input NewTodo {
  ID: String!
  Title: String!
  Description: String!
  Condition: Boolean!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
  deleteTodo(todoId: Int!): Boolean!
}
